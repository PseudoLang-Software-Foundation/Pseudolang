PROCEDURE bubbleSort(aList)
{
    n <- LENGTH(aList)
    REPEAT n TIMES
    {
        j <- 1
        REPEAT n-1 TIMES
        {
            IF(aList[j] > aList[j + 1])
            {
                temp <- aList[j]
                aList[j] <- aList[j + 1]
                aList[j + 1] <- temp
            }
            j <- j + 1
        }
    }
    RETURN (aList)
}

a <- [1, 3, 2, 3, 4, 7, 2]
DISPLAY(bubbleSort(a))